{
    "version": 2,
    "builds": [
        {
            "src": "/server/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server/index.js"
        }
    ],
    "env": {
        "APP_URL": "https://merkle-tree-gift-list.vercel.app"
    }
}
